    </insert>
    <insert id="batchAddObjs" parameterType="java.util.HashMap">
        insert into school_info.special(school_id, special_id, `max`, `min`, average, min_section, info, level1_name,
        level2_name, level3_name, spname, zslx_name, local_batch_name,score_year)
        values
        <foreach collection="list" separator="," item="obj" >
            (#{obj.school_id}, #{obj.special_id}, #{obj.max}, #{obj.min}, #{obj.average},
            #{obj.min_section}, #{obj.info}, #{obj.level1_name}, #{obj.level2_name}, #{obj.level3_name}, #{obj.spname},
            #{obj.zslx_name}, #{obj.local_batch_name},#{obj.score_year})
